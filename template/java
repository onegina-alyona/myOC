import java.util.*;
import java.lang.*;
import java.io.*;

class Ideone
{
	public static long fib(int n) {
		if (n == 0) return 0;
		if (n == 1) return 1;
		return fib(n-1) + fib(n-2);
	}
	
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner in = new Scanner(System.in);
			while(in.hasNextInt()){
			int num = in.nextInt();
			System.out.println(fib(num));
		}
	}
}
